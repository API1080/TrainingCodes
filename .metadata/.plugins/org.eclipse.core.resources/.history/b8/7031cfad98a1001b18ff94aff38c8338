package com.collectiondemo;
interface arithmetic{
	static void adds(int a,int b);
	static int subs(int a,int b) {
		return a-b;
		}
	static int multiply(int a,int b) {
		return a*b;
		}
	static float div(int a,int b) {
		return a+b;
		}
	static int mods(int a,int b) {
		return a%b;
		}
	static int inc(int a) {
		return ++a;
		}
	static int dec(int a) {
		return --a;
		}
}

interface bitwise{
	static int and(int a,int b) {
		return a&b;
		}
	static int or(int a,int b) {
		return a|b;
		}
	static int xor(int a,int b) {
		return a^b;
		}
	static int adds(int a) {
		return ~a;
		}
}

public class ques3 implements arithmetic , bitwise{
	static int adds(int a,int b) {
		return a+b;
		}
	static int subs(int a,int b) {
		return a-b;
		}
	static int multiply(int a,int b) {
		return a*b;
		}
	static float div(int a,int b) {
		return a+b;
		}
	static int mods(int a,int b) {
		return a%b;
		}
	static int inc(int a) {
		return ++a;
		}
	static int dec(int a) {
		return --a;
		}
	
	static int and(int a,int b) {
		return a&b;
		}
	static int or(int a,int b) {
		return a|b;
		}
	static int xor(int a,int b) {
		return a^b;
		}
	static int adds(int a) {
		return ~a;
		}
	
	public static void main(String[] args) {
		ques3 obj=new ques3();
		System.out.println(obj.dec(5));
	}

}
