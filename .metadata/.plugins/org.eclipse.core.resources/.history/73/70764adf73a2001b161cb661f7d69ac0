package com.assign7;
import java.util.*;

public class ques2 {
	public static void main(String[] args) {
		TreeSet<String> al=new TreeSet<String>();  
		al.add("L");  
		al.add("M");  
		al.add("N");  
		al.add("O"); 
		al.add("P");
		al.add("Q"); 
		System.out.println("Initial Set: "+al);
		System.out.print("Reverse Set: "+al.descendingSet());
		System.out.print("elements that are less than or equal to the element ‘O': "+al.headSet("O",true));
		
	}
}
